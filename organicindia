<script>
  function instantBuy(event, anchor){
    var $ = jQuery;
    var form = anchor.closest('form');
    var formParams = $(form).serialize();
    $.ajax({
      url: "/cart/add",
      type: "post",
      data: formParams,
      success: function(){
        window.location.href = "/checkout";
      },
      error: function(){
      }
    })
  }
</script>
