<script type="text/javascript">
    window.appikon = {};

    

    


    

    
    window.appikon.cart = {"note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0.0,"item_count":0,"items":[],"requires_shipping":false,"currency":"INR","items_subtotal_price":0,"cart_level_discount_applications":[]};
    ["requires_shipping", "total_discount", "item_count", "total_weight"].map(function(a) {
        delete window.appikon.cart[a]
    })

    window.appikon.cart.items = [];
    window.appikon.cart_product_ids = [];
    window.appikon.cart_collection_ids = [];
    

    
    window.appikon.adp_page = "";
</script>

